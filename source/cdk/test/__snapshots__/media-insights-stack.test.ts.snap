  system_table = dynamodb_client.Table(SYSTEM_TABLE_NAME)
  config={"Name":"MaxConcurrentWorkflows", "Value":DEFAULT_MAX_CONCURRENT_WORKFLOWS}
  return system_table.put_item(Item=config)
